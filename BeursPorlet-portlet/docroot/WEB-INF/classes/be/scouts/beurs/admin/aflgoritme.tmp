        output.txtPrijs5.Text = CStr(Math.Round(intprijs5, 2)) & " €"
        output.txtPrijs6.Text = CStr(Math.Round(intprijs6, 2)) & " €"
        output.txtPrijs7.Text = CStr(Math.Round(intprijs7, 2)) & " €"
        output.labelMessage.Text = txtMessage.Text
